<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        {% block metadata %}
            {% include 'web/shared/_metadata.html.twig' %}
        {% endblock %}
        
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        
        {% block third_party %}
            {% include 'web/shared/_third_party.html.twig' %}
        {% endblock %}
    </head>
    <body>
        {% include 'web/shared/_flash_messages.html.twig' %}

        <header>
            {% block navbar %}
                {% include 'web/shared/_navbar.html.twig' %}
            {% endblock %}
        </header>

        {% block body %}
                {% block content %}{% endblock %}
        {% endblock %}
    </body>
</html>
