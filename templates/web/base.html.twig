<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="apple-touch-icon" href="{{ asset('build/images/apple-touch-icon.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('build/images/favicon-32x32.png') }}">
    <meta name="theme-color" content="#ffffff">

    {% block metadata %}
        {{ render(path('components_metadata', {
            path: app.request.pathInfo|url_encode
        })) }}
    {% endblock %}

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

    {% include 'web/shared/third_party/index.html.twig' %}
</head>
<body>
{% include 'web/shared/components/_flash_messages.html.twig' %}

<header>
    {% block navbar %}
        {{ render(path('components_navbar')) }}
    {% endblock %}
</header>

{% block body %}
    {% block content %}{% endblock %}
{% endblock %}
</body>
</html>
